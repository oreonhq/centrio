msgid "Estimated Time"
msgstr "Εκτιμώμενη διάρκεια"

msgid "15-30 minutes"
msgstr "15-30 λεπτά"

msgid "Click Next to begin configuration."
msgstr "Κλικ στο Επόμενο για να ξεκινήσει η ρύθμιση."

msgid "Language Changed"
msgstr "Η γλώσσα άλλαξε"

msgid "The language has been changed. Please restart the installer to see the changes."
msgstr "Η γλώσσα άλλαξε. Επανεκκινήστε τον εγκαταστάτη."

msgid "Language Selected"
msgstr "Επιλέχθηκε γλώσσα"

